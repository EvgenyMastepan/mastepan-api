{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/olga.html",
      "dest": "/olga.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
